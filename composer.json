    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        { "name": "Valentin Coulon", "email": "valentin.c0610@gmail.com" },
        { "name": "Jamal Youssefi", "email": "jamal.youssefi@gmail.com" },
        { "name": "Raphael De Freitas", "email": "raphael@de-freitas.net" },
        { "name": "LinkValue", "email": "contact@link-value.fr", "homepage": "http://link-value.fr/" }
